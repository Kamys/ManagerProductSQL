CREATE TABLE attributes
(
    id INTEGER PRIMARY KEY NOT NULL,
    name VARCHAR(255) NOT NULL
);
CREATE TABLE category
(
    id INTEGER PRIMARY KEY NOT NULL,
    name TEXT NOT NULL,
    description TEXT
);
CREATE TABLE goods
(
    id INTEGER PRIMARY KEY NOT NULL,
    cost INTEGER NOT NULL,
    description TEXT
);
CREATE TABLE layouts
(
    id INTEGER PRIMARY KEY NOT NULL,
    name VARCHAR(255) NOT NULL
);
CREATE TABLE leafs
(
    id INTEGER PRIMARY KEY NOT NULL,
    name CHAR(30) NOT NULL,
    id_category INTEGER NOT NULL,
    description TEXT
);
CREATE TABLE link_attribute_and_value
(
    id_attribute INTEGER NOT NULL,
    id_value INTEGER NOT NULL,
    id BIGINT PRIMARY KEY NOT NULL
);
CREATE TABLE link_category_and_category
(
    id INTEGER PRIMARY KEY NOT NULL,
    id_parent INTEGER NOT NULL,
    path LTREE NOT NULL
);
CREATE TABLE link_goods_and_attribute_value
(
    id INTEGER PRIMARY KEY NOT NULL,
    id_goods INTEGER NOT NULL,
    id_attribute_value INTEGER NOT NULL
);
CREATE TABLE link_goods_and_leafs
(
    id INTEGER PRIMARY KEY NOT NULL,
    id_leafs INTEGER NOT NULL,
    id_goods INTEGER NOT NULL
);
CREATE TABLE link_layout_and_attribute
(
    id_layout INTEGER NOT NULL,
    id_attribute INTEGER NOT NULL,
    id BIGINT PRIMARY KEY NOT NULL
);
CREATE TABLE link_layout_and_goods
(
    id INTEGER PRIMARY KEY NOT NULL,
    id_layouts INTEGER NOT NULL,
    id_goods INTEGER NOT NULL
);
CREATE TABLE values
(
    id INTEGER PRIMARY KEY NOT NULL,
    value VARCHAR(255)
);
ALTER TABLE leafs ADD FOREIGN KEY (id_category) REFERENCES category (id);
ALTER TABLE link_attribute_and_value ADD FOREIGN KEY (id_attribute) REFERENCES attributes (id);
ALTER TABLE link_attribute_and_value ADD FOREIGN KEY (id_value) REFERENCES values (id);
CREATE UNIQUE INDEX uk_8xah79ycr6pfi0qawdrikxv2c ON link_attribute_and_value (id_value);
ALTER TABLE link_category_and_category ADD FOREIGN KEY (id_parent) REFERENCES category (id);
ALTER TABLE link_goods_and_attribute_value ADD FOREIGN KEY (id_goods) REFERENCES goods (id);
ALTER TABLE link_goods_and_leafs ADD FOREIGN KEY (id_leafs) REFERENCES leafs (id);
ALTER TABLE link_goods_and_leafs ADD FOREIGN KEY (id_goods) REFERENCES goods (id);
CREATE UNIQUE INDEX link_goods_and_leafs_id_goods_uindex ON link_goods_and_leafs (id_goods);
ALTER TABLE link_layout_and_attribute ADD FOREIGN KEY (id_layout) REFERENCES layouts (id);
ALTER TABLE link_layout_and_attribute ADD FOREIGN KEY (id_attribute) REFERENCES attributes (id);
CREATE UNIQUE INDEX uk_6ogb9k9hr7fhhygwemai04xdl ON link_layout_and_attribute (id_attribute);
ALTER TABLE link_layout_and_goods ADD FOREIGN KEY (id_goods) REFERENCES goods (id);
